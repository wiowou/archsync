Xft*dpi: 96
Xft*hinting: yes
! Xft*hintstyle: 2
!
URxvt.saveLines: 20000
URxvt.font: xft:DejaVu Sans Mono:size=9:antialias=true:hinting=true
URxvt.boldFont: xft:DejaVu Sans Mono:bold:size=9:antialias=true:hinting=true
!URxvt.font: xft:DejaVuSansMono Nerd Font:size=9:antialias=true
!URxvt.boldFont: xft:DejaVuSansMono Nerd Font:style=bold:size=9:antialias=true
URxvt.loginShell: yes
URxvt.scrollstyle: plain
URxvt.scrollbar: false
URxvt.scrollbar_right: false
URxvt.termName: rxvt
URxvt.perl-ext-common: default,selection-to-clipboard,matcher,tabbed
URxvt.matcher.button: 1
URxvt.tabbed.new-button: yes
URxvt.tabbed.reopen-on-close: yes
URxvt.keysym.Control-Right: perl:tabbedex:next_tab
URxvt.keysym.Control-Left: perl:tabbedex:prev_tab
URxvt.keysm.Control-V: eval:paste_clipboard
URxvt.keysm.Control-C: eval:selection_to_clipboard
URxvt.fading: 30
URxvt.transparent: true
URxvt.depth: 32
!URxvt.background: rgba:0000/0000/0200/c800
URxvt.background: rgba:0000/0000/0000/b000
URxvt.shading: 30
URxvt.keysym.C-semicolon: \033[;
!

*background: #000000
*foreground: white
!!*fading: 40
*fadeColor: #002b36
*cursorColor: #93a1a1
*pointerColorBackground: #586e75
*pointerColorForeground: #93a1a1

!! black dark/light
*color0: #073642
*color8: #002b36

!! red dark/light
*color1: #dc322f
*color9: #cb4b16

!! green dark/light
*color2: #859900
*color10: #586e75

!! yellow dark/light
*color3: #b58900
*color11: #657b83

!! blue dark/light
*color4: #268bd2
*color12: #839496

!! magenta dark/light
*color5: #d33682
*color13: #6c71c4

!! cyan dark/light
*color6: #2aa198
*color14: #93a1a1

!! white dark/light
*color7: #eee8d5
*color15: #fdf6e3

